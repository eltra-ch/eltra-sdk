<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"><xs:include schemaLocation="EtherCATInfo.xsd"/>	<!-- Profile Container -->	<xs:element name="ProfileContainer">		<xs:complexType>			<xs:sequence>				<xs:element ref="Profile" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="Profile">		<xs:complexType>			<xs:sequence>				<xs:element ref="ProfileHeader"/>				<xs:element ref="ProfileBody"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<!-- Profile Header -->	<xs:element name="ProfileHeader">		<xs:complexType>			<xs:sequence>				<xs:element ref="ProfileIdentification"/>				<xs:element ref="ProfileRevision"/>				<xs:element ref="ProfileName"/>				<xs:element ref="ProfileSource"/>				<xs:element ref="ProfileClassID"/>				<xs:element name="ProfileDate" type="xs:date" minOccurs="0"/>				<xs:element name="SecurityCode" type="xs:string"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="ProfileIdentification" type="xs:string"/>	<xs:element name="ProfileRevision" type="xs:integer"/>	<xs:element name="ProfileName" type="xs:string"/>	<xs:element name="ProfileSource" type="xs:string"/>	<xs:element name="ProfileClassID" type="xs:NCName"/>	<!-- Profile Body -->	<xs:element name="ProfileBody">		<xs:complexType>			<xs:sequence>				<xs:element ref="DeviceIdentity"/>				<xs:element ref="DeviceManager"/>				<xs:element ref="DeviceHardware"/>				<xs:element ref="DeviceInteraction"/>				<xs:element ref="ApplicationProcess"/>			</xs:sequence>			<xs:attribute name="type" use="required">				<xs:simpleType>					<xs:restriction base="xs:NCName">						<xs:enumeration value="ProfileBody_Device_Maxon"/>						<xs:enumeration value="ProfileBody_Device_Eltra"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="supportedLanguages" type="xs:NMTOKENS" use="required"/>			<xs:attribute name="formatVersion" use="required">				<xs:simpleType>					<xs:restriction base="xs:decimal">						<xs:enumeration value="1.0"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="formatName" use="required">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:enumeration value="EPOS4"/>						<xs:enumeration value="ELTRA"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="fileVersion" use="required">				<xs:simpleType>					<xs:restriction base="xs:decimal">						<xs:enumeration value="1.0"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="fileName" type="xs:string" use="required"/>			<xs:attribute name="fileCreator" type="xs:string" use="required"/>			<xs:attribute name="fileCreationDate" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>    <!-- EsiDevice Definitions -->	<xs:element name="esiDevice">		<xs:complexType>			<xs:complexContent>				<xs:extension base="DeviceType">					<xs:attribute name="uniqueID" type="xs:ID" use="required"/>					<xs:attribute name="Invisible" type="xs:boolean" use="optional"/>					<xs:attribute name="Physics" type="PhysicsType" use="required"/>					<xs:attribute name="Crc32" type="HexDecValue" use="optional"/>				</xs:extension>			</xs:complexContent>		</xs:complexType>	</xs:element>	<xs:element name="esiDeviceIDRef">		<xs:complexType>			<xs:attribute name="uniqueIDRef" type="xs:IDREF" use="required"/>		</xs:complexType>	</xs:element>	<!-- EdsDevice Definitions -->	<xs:element name="edsDeviceDefList">		<xs:complexType>			<xs:sequence>				<xs:element ref="edsDevice" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="edsDevice">		<xs:complexType>			<xs:sequence>				<xs:element ref="edsDeviceInfo" minOccurs="1" maxOccurs="1"/>				<xs:element ref="edsDummyUsage" minOccurs="1" maxOccurs="1"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="edsDeviceIDRef">		<xs:complexType>			<xs:attribute name="uniqueIDRef" type="xs:IDREF" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="edsDeviceInfo">		<xs:complexType>			<xs:sequence>				<xs:element name="baudRate_10" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_20" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_50" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_125" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_250" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_500" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_800" type="xs:boolean" minOccurs="0"/>				<xs:element name="baudRate_1000" type="xs:boolean" minOccurs="0"/>				<xs:element name="simpleBootUpMaster" type="xs:boolean" minOccurs="0"/>				<xs:element name="simpleBootUpSlave" type="xs:boolean" minOccurs="0"/>				<xs:element name="granularity" type="xs:integer" minOccurs="0"/>				<xs:element name="dynamicChannelsSupported" type="xs:boolean" minOccurs="0"/>				<xs:element name="groupMessaging" type="xs:boolean" minOccurs="0"/>				<xs:element name="lssSupported" type="xs:boolean" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="edsDummyUsage">		<xs:complexType>			<xs:sequence>				<xs:element name="dummy0001" type="xs:boolean" minOccurs="0"/>				<xs:element name="dummy0002" type="xs:boolean" minOccurs="0"/>				<xs:element name="dummy0003" type="xs:boolean" minOccurs="0"/>				<xs:element name="dummy0004" type="xs:boolean" minOccurs="0"/>				<xs:element name="dummy0005" type="xs:boolean" minOccurs="0"/>				<xs:element name="dummy0006" type="xs:boolean" minOccurs="0"/>				<xs:element name="dummy0007" type="xs:boolean" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<!-- Device Identity -->	<xs:element name="DeviceIdentity">		<xs:complexType>			<xs:sequence>				<xs:element ref="vendorName" minOccurs="0"/>				<xs:element ref="deviceFamily" minOccurs="0"/>				<xs:element ref="productFamily" minOccurs="0"/>				<xs:element ref="productName" minOccurs="0"/>				<xs:element ref="productText" minOccurs="0"/>				<xs:element ref="productUrl" minOccurs="0"/>				<xs:element ref="productPicture" minOccurs="0" maxOccurs="1"/>				<xs:element ref="orderNumber" minOccurs="0"/>				<xs:element ref="version" minOccurs="0" maxOccurs="4"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="vendorName">		<xs:complexType mixed="true">			<xs:attribute name="readOnly" type="xs:boolean" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="deviceFamily">		<xs:complexType>			<xs:group ref="g_labels"/>			<xs:attribute name="readOnly" type="xs:boolean" default="true"/>		</xs:complexType>	</xs:element>	<xs:element name="productFamily">		<xs:complexType>			<xs:simpleContent>				<xs:extension base="xs:NCName">					<xs:attribute name="readOnly" type="xs:boolean" use="required"/>				</xs:extension>			</xs:simpleContent>		</xs:complexType>	</xs:element>	<xs:element name="instanceName">		<xs:complexType>			<xs:simpleContent>				<xs:extension base="xs:string">					<xs:attribute name="readOnly" type="xs:boolean" default="false"/>				</xs:extension>			</xs:simpleContent>		</xs:complexType>	</xs:element>	<xs:element name="orderNumber">		<xs:complexType>			<xs:simpleContent>				<xs:extension base="xs:string">					<xs:attribute name="readOnly" type="xs:boolean" default="true"/>				</xs:extension>			</xs:simpleContent>		</xs:complexType>	</xs:element>	<xs:element name="productName">		<xs:complexType>			<xs:simpleContent>				<xs:extension base="xs:string">					<xs:attribute name="readOnly" type="xs:boolean" default="true"/>				</xs:extension>			</xs:simpleContent>		</xs:complexType>	</xs:element>	<xs:element name="productText">		<xs:complexType>			<xs:group ref="g_labels"/>			<xs:attribute name="readOnly" type="xs:boolean" default="true"/>		</xs:complexType>	</xs:element>	<xs:element name="productUrl">		<xs:complexType>			<xs:sequence>				<xs:group ref="g_labels" maxOccurs="unbounded"/>			</xs:sequence>			<xs:attribute name="readOnly" type="xs:boolean" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="productPicture">		<xs:complexType mixed="true">			<xs:attribute name="readOnly" type="xs:boolean" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="vendorText">		<xs:complexType>			<xs:group ref="g_labels"/>			<xs:attribute name="readOnly" type="xs:boolean" default="true"/>		</xs:complexType>	</xs:element>	<xs:element name="version">		<xs:complexType>			<xs:simpleContent>				<xs:extension base="xs:string">					<xs:attribute name="versionType" use="required">						<xs:simpleType>							<xs:restriction base="xs:NMTOKEN">								<xs:enumeration value="SW"/>								<xs:enumeration value="HW"/>								<xs:enumeration value="APPNB"/>								<xs:enumeration value="APPVER"/>							</xs:restriction>						</xs:simpleType>					</xs:attribute>					<xs:attribute name="readOnly" type="xs:boolean" default="true"/>				</xs:extension>			</xs:simpleContent>		</xs:complexType>	</xs:element>	<!-- Device Manager -->	<xs:element name="DeviceManager">		<xs:complexType>			<xs:sequence>                <xs:element ref="canOpenInformation" minOccurs="0"/>				<xs:element ref="ethercatSlaveInformation" minOccurs="0"/>				<xs:element ref="deviceDiagnosis" minOccurs="0"/>				<xs:element ref="deviceErrors" minOccurs="0"/>				<xs:element ref="communicationErrors" minOccurs="0"/>				<xs:element ref="inputsOutputsList" minOccurs="0"/>				<xs:element ref="dataRecorderList" minOccurs="0"/>				<xs:element ref="userLevelList"/>			</xs:sequence>		</xs:complexType>	</xs:element>    <xs:element name="ethercatSlaveInformation">		<xs:complexType>			<xs:sequence>				<xs:element ref="esiDevice" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="canOpenInformation">		<xs:complexType>			<xs:sequence>				<xs:element ref="edsDevice"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<!-- Device Diagnosis Definitions -->	<xs:element name="deviceDiagnosis">		<xs:complexType>			<xs:sequence>				<xs:element ref="deviceDiagnosisHistory" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="deviceErrorSolution" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="deviceDiagnosisHistory">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramMaximumMessages" minOccurs="1" maxOccurs="1"/>				<xs:element ref="paramNewestMessage" minOccurs="1" maxOccurs="1"/>				<xs:element ref="paramLastMessage" minOccurs="1" maxOccurs="1"/>				<xs:element ref="paramFirstMessageBuffer" minOccurs="1" maxOccurs="1"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="paramMaximumMessages">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="paramNewestMessage">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="paramLastMessage">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="paramFirstMessageBuffer">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<!-- Device Error Definitions -->	<xs:element name="deviceErrors">		<xs:complexType>			<xs:sequence>				<xs:element ref="deviceErrorHistory" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="deviceErrorSolution" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="deviceErrorHistory">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramNumberOfErrors" minOccurs="1" maxOccurs="1"/>				<xs:element ref="paramFirstErrorBuffer" minOccurs="1" maxOccurs="1"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="paramNumberOfErrors">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="paramFirstErrorBuffer">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="deviceErrorSolution">		<xs:complexType>			<xs:sequence>				<xs:element ref="wizard"/>				<xs:element ref="enumEntryIDRef" minOccurs="1" maxOccurs="unbounded"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="deviceErrorSolutionIDRef">		<xs:complexType>			<xs:attribute name="uniqueIDRef" type="xs:IDREF" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="wizard">		<xs:complexType>			<xs:attribute name="type" use="required">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:enumeration value="diagnostic"/>						<xs:enumeration value="tuning"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>		</xs:complexType>	</xs:element>	<!-- Communication Errors -->	<xs:element name="communicationErrors">		<xs:complexType>			<xs:sequence>				<xs:element ref="communicationError" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="communicationError">		<xs:complexType>			<xs:sequence>				<xs:element ref="errorCode"/>				<xs:group ref="g_labels"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="communicationErrorIDRef">		<xs:complexType>			<xs:attribute name="uniqueIDRef" type="xs:IDREF" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="errorCode" type="xs:string"/>	<!-- Inputs Outputs List -->	<xs:element name="inputsOutputsList">		<xs:complexType>			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:choice>					<xs:element ref="inputsOutputs"/>				</xs:choice>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="inputsOutputs">		<xs:complexType>			<xs:sequence>				<xs:element ref="digitalInput" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="digitalOutput" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="analogInput" minOccurs="0" maxOccurs="unbounded"/>				<xs:element ref="analogOutput" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="digitalInput">		<xs:complexType>			<xs:sequence>				<xs:group ref="g_labels"/>				<xs:element ref="functionalitySelection"/>				<xs:element ref="functionalityState"/>				<xs:element ref="polarity"/>				<xs:element ref="state"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>			<xs:attribute name="id" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="digitalOutput">		<xs:complexType>			<xs:sequence>				<xs:group ref="g_labels"/>				<xs:element ref="functionalitySelection"/>				<xs:element ref="functionalityState"/>				<xs:element ref="polarity"/>				<xs:element ref="state"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>			<xs:attribute name="id" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="analogInput">		<xs:complexType>			<xs:sequence>				<xs:group ref="g_labels"/>				<xs:element ref="functionalitySelection"/>				<xs:element ref="state"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>			<xs:attribute name="id" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="analogOutput">		<xs:complexType>			<xs:sequence>				<xs:group ref="g_labels"/>				<xs:element ref="functionalitySelection"/>				<xs:element ref="functionalityStates" minOccurs="0" maxOccurs="1"/>				<xs:element ref="state"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>			<xs:attribute name="id" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="functionalitySelection">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>				<xs:element ref="varDeclarationIDRef" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="functionalityStates">		<xs:complexType>			<xs:sequence>								<xs:element ref="functionalityState" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="functionalityState">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>				<xs:element ref="varDeclarationIDRef" minOccurs="0"/>			</xs:sequence>			<xs:attribute name="selectionValue" type="xs:integer" use="optional"/>		</xs:complexType>	</xs:element>	<xs:element name="polarity">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>				<xs:element ref="varDeclarationIDRef" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="mask">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>				<xs:element ref="varDeclarationIDRef" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="state">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>				<xs:element ref="varDeclarationIDRef" minOccurs="0"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="value">		<xs:complexType>			<xs:simpleContent>				<xs:extension base="xs:string">					<xs:attribute name="encoding" use="optional">						<xs:simpleType>							<xs:restriction base="xs:string">								<xs:enumeration value="base64"/>								<xs:enumeration value="rawValue"/>								<xs:enumeration value="scaledValue"/>							</xs:restriction>						</xs:simpleType>					</xs:attribute>				</xs:extension>			</xs:simpleContent>		</xs:complexType>	</xs:element>	<!-- DataRecorder List -->	<xs:element name="dataRecorderList">		<xs:complexType>			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:choice>					<xs:element ref="dataRecorder"/>				</xs:choice>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="dataRecorder">		<xs:complexType>			<xs:sequence>				<xs:element ref="recorderTemplateList"/>				<xs:element ref="parameters"/>				<xs:element ref="channels"/>				<xs:element ref="triggerGroupSelection"/>				<xs:element ref="recorderConfigurationList"/>			</xs:sequence>			<xs:attribute name="uniqueID" type="xs:ID" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="parameters">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramSamplingPeriod"/>				<xs:element ref="paramPrecedingSamples"/>				<xs:element ref="paramMaxNbOfSamples"/>				<xs:element ref="paramNbOfRecordedSamples"/>				<xs:element ref="paramBufferTimestamp"/>				<xs:element ref="paramRecorderSnapshot"/>				<xs:element ref="paramRecorderControlWord"/>				<xs:element ref="paramRecorderStatusWord"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="paramBuffer">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="paramSamplingPeriod">		<xs:complexType>			<xs:sequence>				<xs:element ref="paramIDRef"/>			</xs:sequence>		</xs:complexType>	</xs:element>	